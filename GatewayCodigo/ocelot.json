{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7140"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/products",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7217
        }
      ],
      "UpstreamPathTemplate": "/gateway/products",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "GetProducts"
    },
    {
      "DownstreamPathTemplate": "/api/customers",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7217
        }
      ],
      "UpstreamPathTemplate": "/gateway/customers",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "GetCustomers"
    },
    {
      "DownstreamPathTemplate": "/api/categories",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7217
        }
      ],
      "UpstreamPathTemplate": "/gateway/categories",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "GetCategories"
    }
  ],
  "Aggregates": [
    {
      "RouteKeys": [
        "GetProducts",
        "GetCustomers"
      ],
      "UpstreamPathTemplate": "/aggregate/all",
      "UpstreamHttpMethod": [ "Get" ],
      "Aggregator": "SimpleAggregator"
    },
    {
      "RouteKeys": [
        "GetProducts",
        "GetCustomers"
      ],
      "UpstreamPathTemplate": "/aggregate/demo",
      "UpstreamHttpMethod": [ "Get" ],
      "Aggregator": "DemoAggregator"
    },
    {
      "RouteKeys": [
        "GetProducts",
        "GetCategories"
      ],
      "UpstreamPathTemplate": "/aggregate/test",
      "UpstreamHttpMethod": [ "Get" ],
      "Aggregator": "TestAggregator"
    }
  ]
}